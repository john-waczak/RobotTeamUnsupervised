@article{spectral-unmixing-orig,
  author={Keshava, N. and Mustard, J.F.},
  journal={IEEE Signal Processing Magazine}, 
  title={Spectral unmixing}, 
  year={2002},
  volume={19},
  number={1},
  pages={44-57},
  keywords={Hyperspectral sensors;Layout;Reflectivity;Remote sensing;Spectroscopy;Hyperspectral imaging;Spatial resolution;Data mining;Atmospheric modeling;Analytical models},
  doi={10.1109/79.974727}, 
  notes={
      - Mixed pixels
        - low spatial resolution means multiple materials co-occupy a single pixel
        - disparete sources combine into a homogenous mixture
      - Linear Mixing Model
        - Assumption: surface dominated by small number of materials  (endmembers) with consistant spectral properties
        - Two requirements for abundances: nonnegativity and sum-to-one
        - Argument for LMM: at the resolution of georectified images, we can think of pixels as being linear 
          combinations of constituents. Any nonlinear mixtures will have mixing scales with fundamental lengths less thatn the pixel scale.
  }
}

